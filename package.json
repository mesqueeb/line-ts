{
  "name": "line-ts",
  "sideEffects": false,
  "version": "0.0.0",
  "description": "LINE SDK for TypeScript",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "ava",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build.js",
    "build": "npm run lint && npm run test && npm run rollup"
  },
  "engines" : { "node" : ">=10" },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "ava": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^1.32.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "line"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/line-ts/issues"
  },
  "homepage": "https://github.com/mesqueeb/line-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/line-ts.git"
  },
  "ava": {
    "extensions": ["ts"],
    "require": ["tsconfig-paths/register", "ts-node/register"]
  }
}
